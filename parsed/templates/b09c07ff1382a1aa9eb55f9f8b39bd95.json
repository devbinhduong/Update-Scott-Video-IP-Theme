{"components/common/small-paginator":"<div class=\"pagination\">\r\n    <ul class=\"pagination-list pagination-list--small\">\r\n        {{#if previous}}\r\n            <li class=\"pagination-item pagination-item--previous\">\r\n                <a class=\"pagination-link\" href=\"{{previous}}\" data-faceted-search-facet>\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use href=\"#icon-chevron-left\"></use>\r\n                        </svg>\r\n                    </i>\r\n                    {{lang 'common.previous'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        {{#if this.links.length}}\r\n        {{#each this.links}}\r\n            <a class=\"pagination-link\" href=\"{{this.link}}\">\r\n                <li class=\"pagination-item\">{{this.number}}</li>\r\n            </a>\r\n        {{/each}}\r\n        {{/if}}\r\n\r\n        {{#if next}}\r\n            <li class=\"pagination-item pagination-item--next\">\r\n                <a class=\"pagination-link\" href=\"{{next}}\" data-faceted-search-facet>\r\n                    {{lang 'common.next'}}\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use href=\"#icon-chevron-right\"></use>\r\n                        </svg>\r\n                    </i>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</div>\r\n\r\n"}
