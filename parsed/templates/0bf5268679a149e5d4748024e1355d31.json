{"components/custom/global/custom-ask-an-expert-popup":"{{#if page_type '!==' 'page_contact_form'}}\r\n    {{#all (if theme_settings.custom_ask_an_expert) (if theme_settings.custom_ask_an_expert_pagelink)}}\r\n        <div id=\"custom-ask-an-expert\" class=\"custom-popup custom-ask-an-expert-popup modal\" data-reveal>\r\n            <div class=\"modal-content\">\r\n                <div class=\"modal-header modal-header2\">\r\n                    <h2 class=\"modal-header-title\">{{theme_settings.custom_ask_an_expert_title}}</h2>\r\n                    <button class=\"modal-close\" type=\"button\" title=\"Close\">\r\n                        <span class=\"aria-description--hidden\">Close</span>\r\n                        <span aria-hidden=\"true\">Ã—</span>\r\n                    </button>\r\n                </div>\r\n                <div class=\"modal-body customCustomScrollbar\">\r\n                    {{#if theme_settings.custom-ask-an-expert-logo}}\r\n                        <div class=\"custom-ask-an-expert-logo\">\r\n                            <a href=\"/\">\r\n                                {{#if settings.store_logo.image}}\r\n                                    <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n                                {{else}}\r\n                                    <span>{{settings.store_logo.title}}</span>\r\n                                {{/if}}\r\n                            </a>\r\n                        </div>\r\n                    {{/if}}\r\n                    <div class=\"custom-ask-an-expert-subtitle text-center\"><span>{{theme_settings.custom_ask_an_expert_desc1}}</span></div>\r\n                    <div class=\"custom-ask-an-expert-text description text-center\">{{theme_settings.custom_ask_an_expert_desc2}}</div>\r\n                    <div class=\"custom-ask-an-expert-text question text-center\">{{theme_settings.custom_ask_an_expert_desc3}}</div>\r\n                    <div id=\"custom-ask-an-expert-results\" class=\"custom-ask-an-expert-text\"></div>\r\n                    <form data-contact-form id=\"custom-ask-an-expert-form\" class=\"custom-ask-an-expert-form\" action=\"{{urls.contact_us_submit}}\" method=\"post\" data-product-id=\"{{product.id}}\" data-product-title=\"{{product.title}}\" data-product-url=\"{{product.url}}\" data-product-sku=\"{{product.sku}}\">\r\n                        <fieldset class=\"form-fieldset\">\r\n                            <div class=\"hl-form-field-wrapper\"></div>\r\n                            <div class=\"form-field form-field-radio\">\r\n                                <label class=\"form-label\">Do you need:</label>\r\n                                <input class=\"form-radio\" id=\"pricing-radio\" type=\"radio\" name=\"type_package\" checked value=\"Pricing\">\r\n                                <label class=\"form-label\" for=\"pricing-radio\">Pricing</label>\r\n                                <input class=\"form-radio\" id=\"answers-radio\" type=\"radio\" name=\"type_package\" value=\"Answers\">\r\n                                <label class=\"form-label\" for=\"answers-radio\">Answers</label>\r\n                                <input class=\"form-radio\" id=\"both-radio\" type=\"radio\" name=\"type_package\" value=\"Pricing & Answers\">\r\n                                <label class=\"form-label\" for=\"both-radio\">Both</label>\r\n                            </div>\r\n                            <div class=\"form-field\">\r\n                                <label class=\"form-label\" for=\"contact_comment_area\">What can i help you with today?\r\n                                    <small>*</small>\r\n                                </label>\r\n                                <textarea name=\"contact_comment_area\" cols=\"30\" rows=\"6\" class=\"form-input\" id=\"contact_comment_area\" required></textarea>\r\n                            </div>\r\n                            <div class=\"form-field form-field-radio form-field-radio2\">\r\n                                <label class=\"form-label form-label-custom\">How would you like me to contact you?</label>\r\n                                <input class=\"form-radio\" id=\"phone-radio\" type=\"radio\" name=\"type_contact\" checked value=\"Phone\">\r\n                                <label class=\"form-label\" for=\"phone-radio\">Phone</label>\r\n                                <input class=\"form-radio\" id=\"email-radio\" type=\"radio\" name=\"type_contact\" value=\"Email\">\r\n                                <label class=\"form-label\" for=\"email-radio\">Email</label>\r\n                                <input class=\"form-radio\" id=\"both-radio-2\" type=\"radio\" name=\"type_contact\" value=\"Phone & Email\">\r\n                                <label class=\"form-label\" for=\"both-radio-2\">Both</label>\r\n                            </div>\r\n                            <div class=\"hl-form-field\" style=\"display: none;\">\r\n                                <textarea name=\"contact_question\" cols=\"30\" rows=\"3\" class=\"form-input\" id=\"contact_question\"></textarea>\r\n                            </div>\r\n                            <div class=\"g-recaptcha\" data-sitekey=\"6LcjX0sbAAAAACp92-MNpx66FT4pbIWh-FTDmkkz\"></div>\r\n                            <div class=\"form-actions\">\r\n                                <button type=\"submit\" class=\"button button--tertiary\" id=\"custom-ask-an-expert-button\"><span>Send Question</span></button>\r\n                             </div>\r\n                        </fieldset>\r\n                    </form>\r\n                    <script src=\"https://www.google.com/recaptcha/api.js\" async=\"\" defer=\"\"></script>\r\n\r\n                </div>\r\n            </div>\r\n        </div>\r\n    {{/all}}\r\n{{/if}}\r\n"}
