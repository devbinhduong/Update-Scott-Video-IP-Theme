e -->\r\n                     <a class=\"navUser-action myAccountMobile hide-on-desktop\" {{#if customer}}href=\"{{urls.account.index}}\"{{else}}href=\"{{urls.auth.login}}\"{{/if}} data-login-form-mobile aria-label=\"{{lang 'account.title'}}\">\r\n                        <span class=\"navUser-icon\">\r\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-information\"></use></svg>\r\n                        </span>\r\n                    </a>\r\n                </div>\r\n\r\n                <div class=\"items item--cart custom-cart\">\r\n                    <!-- Cart Icon for Desktop -->\r\n                    <a class=\"navUser-action hide-on-tablet hover-link\"\r\n                        style=\"--underlineColor:{{theme_settings.navUser-color-hover}}\"\r\n                        data-cart-preview\r\n                        href=\"{{urls.cart}}\"\r\n                        aria-label=\"{{lang 'cart.nav_aria_label'}}\">\r\n                        <span class=\"navUser-icon navUser-item-cartIcon\">\r\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-shopping-cart\"></use></svg>\r\n                        </span>\r\n                        <span>{{lang 'common.cart'}}</span>\r\n                    </a>\r\n                    <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity hide-on-tablet header-countPill\">{{cart.quantity}}</span>\r\n                    <div class=\"dropdown-cart\" id=\"cart-preview-dropdown\" aria-hidden=\"true\"></div>\r\n\r\n                    <!-- Cart Icon for Mobile -->\r\n                    <a class=\"navUser-action cartMobile hide-on-desktop\" data-cart-preview2 href=\"/cart.php\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\r\n                        <span class=\"navUser-icon navUser-item-cartIcon\">\r\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-shopping-cart\"></use></svg>\r\n                            <span class=\"countPill cart-quantity header-countPill\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\r\n                        </span>\r\n                        {{lang 'common.cart'}}\r\n                    </a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- Header Bottom -->\r\n    <div class=\"headerBottom\">\r\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n            {{> components/common/navigation-menu}}\r\n        </div>\r\n    </div>\r\n    {{{region name=\"header_navigation_bottom--global\"}}}\r\n</header>\r\n{{{region name=\"header_bottom--global\"}}}\r\n{{{region name=\"header_bottom\"}}}\r\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal level-2\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <div class=\"container\">\r\n        <ul class=\"navPage-subMenu-list\">\r\n            <li class=\"navPage-subMenu-item-parent navPage-subMenu-item-child navPage-subMenu-title\">\r\n                <p class=\"navPage-subMenu-action navPages-action navPages-action-lv1 navPages-action-depth-max has-subMenu\">\r\n                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                        <svg class=\"icon\"><use href=\"#icon-long-arrow-left\" /></svg>\r\n                    </span>\r\n                    <a class=\"text hover-link\" href=\"{{url}}\"><span>{{name}}</span></a>\r\n                </p>\r\n            </li>\r\n            <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all navPage-subMenu-all--lv1 custom-fadeInLeft\" data-step-animate=\"0\">\r\n                <a class=\"navPage-subMenu-action navPages-action navPages-action-lv1 no-subMenu hover-link\" href=\"{{url}}\">\r\n                    <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\r\n                </a>\r\n            </li>\r\n            {{#each children}}\r\n                {{#if children}}\r\n                    <li class=\"navPage-subMenu-item-child has-dropdown custom-fadeInLeft\" data-step-animate=\"0\">\r\n                        <p class=\"navPage-subMenu-action navPages-action navPages-action-lv2 navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" data-navId=\"navPages-{{id}}\">\r\n                            <a class=\"text hover-link\" href=\"{{url}}\"><span>{{name}}</span></a>\r\n                            <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                <svg class=\"icon\"><use href=\"#icon-chevron-down\" /></svg>\r\n                            </span>\r\n                        </p>\r\n                        <div class=\"navPage-subMenu navPage-subMenu-horizontal level-3\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                            <ul class=\"navPage-subMenu-list\">\r\n                                <li class=\"navPage-subMenu-item-parent navPage-subMenu-item-child navPage-subMenu-title\">\r\n                                    <p class=\"navPage-subMenu-action navPages-action navPages-action-lv2 {{#if is_active}} activePage{{/if}}\">\r\n                                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                            <svg class=\"icon\"><use href=\"#icon-long-arrow-left\" /></svg>\r\n                                        </span>\r\n                                        <a class=\"text hover-link\" href=\"{{url}}\"><span>{{name}}</span></a>\r\n                                    </p>\r\n                                </li>\r\n                                <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all navPage-subMenu-all--lv2\">\r\n                                    <a class=\"navPage-subMenu-action navPages-action navPages-action-lv2 no-subMenu {{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\r\n                                        <span class=\"text hover-link\">{{lang 'category.view_all.name' category=name}}</span>\r\n                                    </a>\r\n                                </li>\r\n                                {{#each children}}\r\n                                    <li class=\"navPage-subMenu-item-child navPages-action-end\">\r\n                                        <a class=\"navPage-subMenu-action navPages-action navPages-action-lv3 no-subMenu level-3 hover-link{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\r\n                                            <span class=\"text\">{{name}}</span>\r\n                                        </a>\r\n                                    </li>\r\n                                {{/each}}\r\n                            </ul>\r\n                        </div>\r\n                    </li>\r\n                {{else}}\r\n                    <li class=\"navPage-subMenu-item-child navPages-action-end custom-fadeInLeft\" data-step-animate=\"0\">\r\n                        <a class=\"navPage-subMenu-action navPages-action navPages-action-lv2 no-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text hover-link\">{{name}}</span></a>\r\n                    </li>\r\n                {{/if}}\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n</div>\r\n","components/common/navigation-list":"{{#if children}}\r\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\r\n   href=\"{{url}}\"\r\n   data-collapsible=\"navPages-{{id}}\"\r\n>\r\n    {{name}}\r\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n        <svg><use href=\"#icon-chevron-down\" /></svg>\r\n    </i>\r\n</a>\r\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        <li class=\"navPage-subMenu-item\">\r\n            <a class=\"navPage