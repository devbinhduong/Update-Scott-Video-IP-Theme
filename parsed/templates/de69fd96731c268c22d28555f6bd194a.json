}}\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action navPages-action-lv1 no-subMenu navPages-action-end hover-link\" data-label=\"/brands/\" href=\"/brands/\" aria-label=\"Brands\">\r\n                <span class=\"text\">Brands</span>\r\n            </a>\r\n        </li>\r\n        \r\n        {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{/if}}\">\r\n                    {{> components/common/navigation-list-alternate page = ../page type = 'page'}}\r\n                </li>\r\n            {{/each}}\r\n        {{/unless}}\r\n    </ul>\r\n</nav>\r\n","components/common/quick-search":"<div class=\"container\">\r\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\"\r\n                        id=\"search_query\"\r\n                       data-search-quick\r\n                       name=\"{{name}}\"\r\n                       id=\"{{name}}\"\r\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\r\n                       autocomplete=\"off\"\r\n                >\r\n            </div>\r\n        </fieldset>\r\n\r\n        <button class=\"search-header-button\" type=\"submit\" data-search-quick-submit>\r\n            <span class=\"is-srOnly\">Search Button</span>\r\n            <span class=\"icon icon--search\">\r\n                <svg><use xlink:href=\"#icon-search\"></use></svg>\r\n            </span>\r\n        </button>\r\n    </form>\r\n    <section class=\"quickSearchResults customQuickSearchResults\" data-bind=\"html: results\"></section>\r\n\r\n    <!-- Custom Start -->\r\n    {{#if theme_settings.beforeQuickSearch}}\r\n        <div class=\"customQuickSearchResults quickSearchResultsCustom\">\r\n            <div class=\"container\">\r\n                <button class=\"quickResults-close\" type=\"button\" title=\"Close\">\r\n                    <span class=\"aria-description--hidden\">Close</span>\r\n                    <span aria-hidden=\"true\">×</span>\r\n                </button>\r\n            </div>\r\n            <div class=\"container\">\r\n                {{> components/custom/custom-before-quick-results  productsnew=products.new }}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n    \r\n    {{~inject 'quickSearchBefore' theme_settings.beforeQuickSearch}}\r\n\r\n</div>\r\n","components/common/store-logo":"<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link rel=\"preload\" as=\"image\">\r\n    {{#if settings.store_logo.image}}\r\n        {{#if theme_settings.logo_size '===' 'original'}}\r\n        <img class=\"header-logo-image-unknown-size\"\r\n             src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\r\n             alt=\"{{settings.store_logo.title}}\"\r\n             title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n        <div class=\"header-logo-image-container\">\r\n            <img class=\"header-logo-image\"\r\n                 src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\r\n                 srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\r\n                 alt=\"{{settings.store_logo.title}}\"\r\n                 title=\"{{settings.store_logo.title}}\">\r\n        </div>\r\n        {{/if}}\r\n    {{else}}\r\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n","components/custom/custom-account-popup":"<div id=\"{{data}}-popup\" class=\"account-popup{{#if data '==' 'account-pc'}} custom-auth-popup{{/if}}\">\r\n    <div class=\"custom-popup-content\">\r\n        <ul class=\"account-dropdown\">\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.orders.all}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.orders'}}</span>\r\n                </a>\r\n            </li>\r\n            {{#if settings.returns_enabled}}\r\n                <li class=\"account-item\">\r\n                    <a class=\"account-action hover-link\" href=\"{{urls.account.returns}}\">\r\n                        <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.returns'}}</span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.inbox}}\">\r\n                    <span class=\"hover-link--account-dropdown\">Messages</span>\r\n                </a>\r\n            </li>\r\n                <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.addresses}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.addresses'}}</span>\r\n                </a>\r\n            </li>\r\n            {{#if settings.show_payment_methods}}\r\n                <li class=\"account-item\">\r\n                    <a class=\"account-action hover-link\" href=\"{{urls.account.payment_methods.all}}\">\r\n                        <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.payment_methods'}}</span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n            {{#if settings.show_wishlist}}\r\n                <li class=\"account-item\">\r\n                    <a class=\"account-action hover-link\" href=\"{{urls.account.wishlists.all}}\">\r\n                        <span class=\"hover-link--account-dropdown\">Wishlist</span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.recent_items}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.recently_viewed'}}</span>\r\n                </a>\r\n            </li>\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.details}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.settings'}}</span>\r\n                </a>\r\n            </li>\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.auth.logout}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'common.logout'}}</span>\r\n                </a>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n</div>\r\n","components/custom/custom-before-quick-results":"<div class=\"quickResults-wrapper\">\r\n    {{#if theme_settings.beforeQuickSearch-trending-now}}\r\n        <div class=\"quickResults-item quickResults-text\">\r\n            {{#if theme_settings.beforeQuickSearch-trending-now-title}}\r\n                <h3 class=\"quickResults-title\">{{theme_settings.beforeQuickSearch-trending-now-title}}</h3>\r\n            {{/if}}\r\n            <ul class=\"productGrid-search\">\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-1}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-1)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-1}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-2}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-2)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-2}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-3}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-3)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-3}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-4}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-4)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-4}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-5}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-5)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-5}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n            </ul>\r\n        </div>\r\n    {{/if}}\r\n    {{#if theme_settings.beforeQuickSearch-popular}}\r\n        <div class=\"quickResults-item quickResults-product\">\r\n            {{#if theme_settings.beforeQuickSearch-popular-title}}\r\n                <h3 class=\"quickResults-title\">{{theme_settings.beforeQuickSearch-popular-title}}</h3>\r\n            {{/if}}\r\n            <div class=\"loadingOverlay\"></div>\r\n            <ul class=\"productGrid\"></ul>\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n{{~inject 'quickSearchPopularId' theme_settings.beforeQuickSearch-popular-id}}\r\n{{~inject 'quickSearchResultLimit' theme_settings.beforeQuickSearch-limit}}\r\n","components/custom/custom-login-popup":"<div id=\"{{data}}-popup\" class=\"login-form-popup{{#if data '==' 'login-pc'}} custom-auth-popup{{/if}}\">\r\n    <div class=\"custom-popup-content\">\r\n        <form class=\"form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\r\n            <div class=\"login-form-wrapper\">\r\n                <div class=\"form-field\">\r\n                    <label class=\"form-label\" for=\"{{email_id}}\">\r\n                        {{lang 'login.field_email'}}\r\n                        <em class=\"text-danger\">*</em>\r\n                    </label>\r\n                    <input id=\"{{email_id}}\" class=\"form-input\" type=\"email\" value=\"\" name=\"login_email\" placeholder=\"{{lang 'login.field_email'}}\">\r\n                </div>\r\n                <div class=\"form-field\">\r\n                    <label class=\"form-label\" for=\"{{password_id}}\">\r\n                        {{lang 'login.field_password'}}\r\n                        <em class=\"text-danger\">*</em>\r\n                    </label>\r\n                    <input id=\"{{password_id}}\" class=\"form-input\" type=\"password\" value=\"\" placeholder=\"{{lang 'login.field_password'}}\" name=\"login_pass\" autocomplete=\"on\">\r\n                </div>\r\n                <div class=\"form-actions text-center\">\r\n                    <button type=\"button\" class=\"button button--primary button--action\">{{lang 'login.submit_value' }}</button>\r\n                    <a class=\"forgot-password hover-link\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password'}}</a>\r\n                    <a class=\"button button--transparent button--action\" href=\"{{urls.auth.create_account}}\">{{lang 'login.new_customer.create_account'}}</a>\r\n                </div>\r\n            </div>\r\n        </form>\r\n    </div>\r\n</div>\r\n","components/custom/header/header-top":"<div class=\"headerTop\">\r\n    <div class=\"container\">\r\n        <div class=\"flex flex-between flex-center-y-tablet\">\r\n            {{#if settings.phone_number}}\r\n                <div class=\"headerTop__left hide-on-tablet\">\r\n                    <p>{{settings.phone_number}}</p>\r\n                </div>\r\n            {{/if}}\r\n\r\n            <div class=\"headerTop__center\">\r\n                <p class=\"text-center-tablet\">\r\n                    {{theme_settings.header_top_center_text}}\r\n                </p>\r\n            </div>\r\n\r\n            <div class=\"headerTop__right hide-on-tablet\">\r\n                <ul class=\"flex\" style=\"--underlineColor: {{theme_settings.header_top_text_colorHover}}\">\r\n                    {{#if theme_settings.header_right_item1_text}}\r\n                        <li>\r\n                            <a class=\"hover-link\" href=\"{{theme_settings.header_right_item1_link}}\">\r\n                                <span>{{theme_settings.header_right_item1_text}}</span>\r\n                            </a>\r\n                        </li>\r\n                    {{/if}}\r\n                    {{#if theme_settings.header_right_item2_text}}\r\n                        <li>\r\n                            <a class=\"hover-link\" href=\"{{theme_settings.header_right_item2_link}}\">\r\n                                <span>{{theme_settings.header_right_item2_text}}</span>\r\n                            </a>\r\n                        </li>\r\n                    {{/if}}\r\n                    \r\n                    {{#if theme_settings.header_right_item3_text}}\r\n                        <li>\r\n                            <a class=\"hover-link\" href=\"{{theme_settings.header_right_item3_link}}\">\r\n                                <span class=\"icon icon--help\">\r\n                                    <svg><use xlink:href=\"#icon-help\"></use></svg>\r\n                                </span>\r\n                                <span>{{theme_settings.header_right_item3_text}}</span> \r\n                            </a>\r\n                        </li>\r\n                    {{/if}}\r\n                    {{#if theme_settings.header_right_item4_text}}\r\n                        <li>\r\n                            <a class=\"hover-link\"href=\"{{theme_settings.header_right_item4_link}}\">\r\n                                <span class=\"icon icon--chat\">\r\n                                    <svg><use xlink:href=\"#icon-chat\"></use></svg>\r\n                                </span>\r\n                                <span>{{theme_settings.header_right_item4_text}}</span>\r\n                            </a>\r\n                        </li>\r\n                    {{/if}}\r\n                </ul>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\n"}
