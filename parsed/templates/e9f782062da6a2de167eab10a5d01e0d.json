{"components/common/navigation-mobile":"<nav class=\"navPages\" role=\"navigation\" aria-label=\"Main Navigation\">\r\n    <ul class=\"navPages-list navPages-list-mobile navPages-list-custom\"></ul>\r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        <li class=\"navPages-item navPages-item-user\">\r\n            {{#if customer}}\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{urls.account.index}}\" aria-label=\"{{lang 'account.title'}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-user\"></use>\r\n                        </svg>\r\n                        {{lang 'account.title'}}\r\n                    </span>\r\n                </a>\r\n            {{else}}\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'login.title'}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-user\"></use>\r\n                        </svg>\r\n                        {{lang 'login.title'}}\r\n                    </span>\r\n                </a>\r\n            {{/if}}\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-user\">\r\n            <a class=\"navPages-action navPages-action-end\" href=\"/wishlist.php\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\r\n                <span class=\"text\">\r\n                    <svg class=\"icon\" aria-hidden=\"true\">\r\n                        <use xlink:href=\"#icon-heart\"></use>\r\n                    </svg>\r\n                    {{lang 'account.wishlists.heading'}}\r\n                </span>\r\n            </a>\r\n        </li>\r\n        {{#if settings.phone_number}}\r\n            <li class=\"navPages-item navPages-item-user\">\r\n                <a class=\"navPages-action navPages-action-end\" href=\"tel:{{settings.phone_number}}\" aria-label=\"Phone number {{settings.phone_number}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-phone\"></use>\r\n                        </svg>\r\n                        {{settings.phone_number}}\r\n                    </span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        \r\n        {{#if theme_settings.header_right_item1_text}}\r\n            <li class=\"navPages-item navPages-item-user\">\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{theme_settings.header_right_item1_link}}\" aria-label=\"{{theme_settings.header_right_item1_text}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-about-us\"></use>\r\n                        </svg>\r\n                        {{theme_settings.header_right_item1_text}}\r\n                    </span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        {{#if theme_settings.header_right_item2_text}}\r\n            <li class=\"navPages-item navPages-item-user\">\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{theme_settings.header_right_item2_link}}\" aria-label=\"{{theme_settings.header_right_item2_text}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-blog\"></use>\r\n                        </svg>\r\n                        {{theme_settings.header_right_item2_text}}\r\n                    </span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        {{#if theme_settings.header_right_item3_text}}\r\n            <li class=\"navPages-item navPages-item-user\">\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{theme_settings.header_right_item3_link}}\" aria-label=\"{{theme_settings.header_right_item3_text}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-help2\"></use>\r\n                        </svg>\r\n                        {{theme_settings.header_right_item3_text}}\r\n                    </span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        {{#if theme_settings.header_right_item4_text}}\r\n            <li class=\"navPages-item navPages-item-user\">\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{theme_settings.header_right_item4_link}}\" aria-label=\"{{theme_settings.header_right_item4_text}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-chat2\"></use>\r\n                        </svg>\r\n                        {{theme_settings.header_right_item4_text}}\r\n                    </span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item navPages-item-user has-dropdown\">\r\n                <p class=\"navPages-action navPages-action-depth-max has-subMenu is-root\">\r\n                    <a class=\"text\" href=\"#\">\r\n                        <span class=\"flag\">{{{currency_selector.active_currency_flag}}}</span> \r\n                        {{currency_selector.active_currency_code}}\r\n                    </a>\r\n                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                    </span>\r\n                </p>\r\n                <div class=\"navPage-subMenu navPage-subMenu-horizontal\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\r\n                            <p class=\"navPage-subMenu-action navPages-action\">\r\n                                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                                    <span>back</span>\r\n                                </span>\r\n                                <a class=\"text\" href=\"#\">\r\n                                    {{currency_selector.active_currency_code}}\r\n                                </a>\r\n                            </p>\r\n                        </li>\r\n                        {{#each currency_selector.currencies}}\r\n                            <li class=\"navPage-subMenu-item-child navPages-action-end\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                    <span class=\"text\"><span class=\"flag\">{{{flag}}}</span>{{name}}</span>\r\n                                </a>\r\n                            </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.show_social-links}}\r\n            <li class=\"navPages-item navPages-item-user\">\r\n                <div class=\"navPages-action navPages-action-end\">\r\n                    <div class=\"text\">\r\n                        {{> components/common/social-links position=\"newsletter\"}}\r\n                    </div>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n","components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            {{#if display_name '===' 'twitter'}}\r\n                <li class=\"socialLinks-item\">\r\n                    <a class=\"icon icon--x\"\r\n                    href=\"{{url}}\"\r\n                    target=\"_blank\"\r\n                    rel=\"noopener\"\r\n                    title=\"X\"\r\n                    >\r\n                        <span class=\"aria-description--hidden\">X</span>\r\n                        <svg>\r\n                            <use href=\"#icon-x\"/>\r\n                        </svg>\r\n                    </a>\r\n                </li>\r\n            {{else}}\r\n                <li class=\"socialLinks-item\">\r\n                    <a class=\"icon icon--{{display_name}}\"\r\n                    href=\"{{url}}\"\r\n                    target=\"_blank\"\r\n                    rel=\"noopener\"\r\n                    title=\"{{{capitalize display_name}}}\"\r\n                    >\r\n                        <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\r\n                        <svg>\r\n                            <use href=\"#icon-{{display_name}}\"/>\r\n                        </svg>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n","components/custom/custom-account-popup":"<div id=\"{{data}}-popup\" class=\"account-popup{{#if data '==' 'account-pc'}} custom-auth-popup{{/if}}\">\r\n    <div class=\"custom-popup-content\">\r\n        <ul class=\"account-dropdown\">\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.orders.all}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.orders'}}</span>\r\n                </a>\r\n            </li>\r\n            {{#if settings.returns_enabled}}\r\n                <li class=\"account-item\">\r\n                    <a class=\"account-action hover-link\" href=\"{{urls.account.returns}}\">\r\n                        <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.returns'}}</span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.inbox}}\">\r\n                    <span class=\"hover-link--account-dropdown\">Messages</span>\r\n                </a>\r\n            </li>\r\n                <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.addresses}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.addresses'}}</span>\r\n                </a>\r\n            </li>\r\n            {{#if settings.show_payment_methods}}\r\n                <li class=\"account-item\">\r\n                    <a class=\"account-action hover-link\" href=\"{{urls.account.payment_methods.all}}\">\r\n                        <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.payment_methods'}}</span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n            {{#if settings.show_wishlist}}\r\n                <li class=\"account-item\">\r\n                    <a class=\"account-action hover-link\" href=\"{{urls.account.wishlists.all}}\">\r\n                        <span class=\"hover-link--account-dropdown\">Wishlist</span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.recent_items}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.recently_viewed'}}</span>\r\n                </a>\r\n            </li>\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.account.details}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'account.nav.settings'}}</span>\r\n                </a>\r\n            </li>\r\n            <li class=\"account-item\">\r\n                <a class=\"account-action hover-link\" href=\"{{urls.auth.logout}}\">\r\n                    <span class=\"hover-link--account-dropdown\">{{lang 'common.logout'}}</span>\r\n                </a>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n</div>\r\n","components/custom/custom-login-popup":"<div id=\"{{data}}-popup\" class=\"login-form-popup{{#if data '==' 'login-pc'}} custom-auth-popup{{/if}}\">\r\n    <div class=\"custom-popup-content\">\r\n        <form class=\"form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\r\n            <div class=\"login-form-wrapper\">\r\n                <div class=\"form-field\">\r\n                    <label class=\"form-label\" for=\"{{email_id}}\">\r\n                        {{lang 'login.field_email'}}\r\n                        <em class=\"text-danger\">*</em>\r\n                    </label>\r\n                    <input id=\"{{email_id}}\" class=\"form-input\" type=\"email\" value=\"\" name=\"login_email\" placeholder=\"{{lang 'login.field_email'}}\">\r\n                </div>\r\n                <div class=\"form-field\">\r\n                    <label class=\"form-label\" for=\"{{password_id}}\">\r\n                        {{lang 'login.field_password'}}\r\n                        <em class=\"text-danger\">*</em>\r\n                    </label>\r\n                    <input id=\"{{password_id}}\" class=\"form-input\" type=\"password\" value=\"\" placeholder=\"{{lang 'login.field_password'}}\" name=\"login_pass\" autocomplete=\"on\">\r\n                </div>\r\n                <div class=\"form-actions text-center\">\r\n                    <button type=\"button\" class=\"button button--primary button--action\">{{lang 'login.submit_value' }}</button>\r\n                    <a class=\"forgot-password hover-link\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password'}}</a>\r\n                    <a class=\"button button--transparent button--action\" href=\"{{urls.auth.create_account}}\">{{lang 'login.new_customer.create_account'}}</a>\r\n                </div>\r\n            </div>\r\n        </form>\r\n    </div>\r\n</div>\r\n","components/custom/sidebar/custom-sidebar":"<!-- Menu Mobile Sidebar -->\r\n<div class=\"custom-sidebar custom-menu-mobile custom-sidebar-left animated{{#if theme_settings.customMegamenu}} custom-menu-megamenu{{/if}}\" id=\"custom-menu-mobile\">\r\n    <a href=\"#\" class=\"custom-sidebar-close\" aria-label=\"Close Menu Sidebar\">\r\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\r\n    </a>\r\n    <div class=\"custom-sidebar-wrapper navPages-container\" data-menu>\r\n        {{> components/common/navigation-mobile }}\r\n    </div>\r\n</div>\r\n\r\n<!-- Search Mobile Sidebar -->\r\n<div class=\"custom-sidebar custom-search-mobile custom-sidebar-left\" id=\"custom-search-mobile\">\r\n    <a href=\"#\" class=\"custom-sidebar-close\" aria-label=\"Close Search Sidebar\">\r\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\r\n    </a>\r\n    <div class=\"custom-sidebar-wrapper\"></div>\r\n</div>\r\n\r\n<!--  Cart Sidebar -->\r\n<div class=\"custom-sidebar custom-cart-sidebar custom-sidebar-right\" id=\"custom-cart-sidebar\">\r\n    <a href=\"#\" class=\"custom-sidebar-close\" aria-label=\"Close Cart Sidebar\">\r\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\r\n    </a>\r\n    <div class=\"custom-sidebar-wrapper\"></div>\r\n</div>\r\n\r\n<!-- Account Sidebar -->\r\n{{#if page_type '!=' 'login'}}\r\n    <div class=\"custom-sidebar custom-auth-sidebar custom-sidebar-right\" id=\"{{#if customer}}custom-account-mobile{{else}}custom-login-mobile{{/if}}\">\r\n        <a href=\"#\" class=\"custom-sidebar-close\" aria-label=\"Close Account Sidebar\">\r\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\r\n        </a>\r\n        <div class=\"custom-sidebar-wrapper\">\r\n            {{#if customer}}\r\n                {{> components/custom/custom-account-popup data=\"account-mb\"}}\r\n            {{else}}\r\n                {{> components/custom/custom-login-popup data=\"login-mb\" email_id=\"login_email2\" password_id=\"login_pass2\"}}\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n{{/if}}\n"}
