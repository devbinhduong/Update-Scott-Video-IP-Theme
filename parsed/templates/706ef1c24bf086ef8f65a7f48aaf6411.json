{"components/common/quick-search":"<div class=\"container\">\r\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\"\r\n                        id=\"search_query\"\r\n                       data-search-quick\r\n                       name=\"{{name}}\"\r\n                       id=\"{{name}}\"\r\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\r\n                       autocomplete=\"off\"\r\n                >\r\n            </div>\r\n        </fieldset>\r\n\r\n        <button class=\"search-header-button\" type=\"submit\" data-search-quick-submit>\r\n            <span class=\"is-srOnly\">Search Button</span>\r\n            <span class=\"icon icon--search\">\r\n                <svg><use xlink:href=\"#icon-search\"></use></svg>\r\n            </span>\r\n        </button>\r\n    </form>\r\n    <section class=\"quickSearchResults customQuickSearchResults\" data-bind=\"html: results\"></section>\r\n\r\n    <!-- Custom Start -->\r\n    {{#if theme_settings.beforeQuickSearch}}\r\n        <div class=\"customQuickSearchResults quickSearchResultsCustom\">\r\n            <div class=\"container\">\r\n                <button class=\"quickResults-close\" type=\"button\" title=\"Close\">\r\n                    <span class=\"aria-description--hidden\">Close</span>\r\n                    <span aria-hidden=\"true\">Ã—</span>\r\n                </button>\r\n            </div>\r\n            <div class=\"container\">\r\n                {{> components/custom/custom-before-quick-results  productsnew=products.new }}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n    \r\n    {{~inject 'quickSearchBefore' theme_settings.beforeQuickSearch}}\r\n\r\n</div>\r\n","components/custom/custom-before-quick-results":"<div class=\"quickResults-wrapper\">\r\n    {{#if theme_settings.beforeQuickSearch-trending-now}}\r\n        <div class=\"quickResults-item quickResults-text\">\r\n            {{#if theme_settings.beforeQuickSearch-trending-now-title}}\r\n                <h3 class=\"quickResults-title\">{{theme_settings.beforeQuickSearch-trending-now-title}}</h3>\r\n            {{/if}}\r\n            <ul class=\"productGrid-search\">\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-1}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-1)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-1}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-2}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-2)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-2}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-3}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-3)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-3}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-4}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-4)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-4}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.beforeQuickSearch-trending-now-5}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.beforeQuickSearch-trending-now-5)}}&section=product\">\r\n                            {{theme_settings.beforeQuickSearch-trending-now-5}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n            </ul>\r\n        </div>\r\n    {{/if}}\r\n    {{#if theme_settings.beforeQuickSearch-popular}}\r\n        <div class=\"quickResults-item quickResults-product\">\r\n            {{#if theme_settings.beforeQuickSearch-popular-title}}\r\n                <h3 class=\"quickResults-title\">{{theme_settings.beforeQuickSearch-popular-title}}</h3>\r\n            {{/if}}\r\n            <div class=\"loadingOverlay\"></div>\r\n            <ul class=\"productGrid\"></ul>\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n{{~inject 'quickSearchPopularId' theme_settings.beforeQuickSearch-popular-id}}\r\n{{~inject 'quickSearchResultLimit' theme_settings.beforeQuickSearch-limit}}\r\n"}
