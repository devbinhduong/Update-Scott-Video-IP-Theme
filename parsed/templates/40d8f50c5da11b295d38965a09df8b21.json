{"components/common/alert/alert-modal":"<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\r\n    <div class=\"alert-icon error-icon\">\r\n        <span class=\"icon-content\">\r\n            <span class=\"line line-left\"></span>\r\n            <span class=\"line line-right\"></span>\r\n        </span>\r\n    </div>\r\n\r\n    <div class=\"alert-icon warning-icon\">\r\n        <div class=\"icon-content\">!</div>\r\n    </div>\r\n\r\n    <div class=\"modal-content\"></div>\r\n\r\n    <div class=\"button-container button-container--custom\">\r\n        <button type=\"button\" class=\"confirm button button button--primary button--action\" data-reveal-close>{{lang 'common.ok'}}</button>\r\n        <button type=\"button\" class=\"cancel button button button--transparent button--action\" data-reveal-close>{{lang 'common.cancel'}}</button>\r\n    </div>\r\n</div>\r\n","components/common/body":"<div class=\"body\" id=\"main-content\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n    \r\n    {{#or (if page_type '===' 'default') (if page_type '===' 'category') (if page.title '===' 'About Us') (if page.title '===' 'Contact Us')}}\r\n        {{#block \"page\"}} {{/block}}\r\n    {{else}}\r\n        <div class=\"container\">\r\n            {{#block \"page\"}} {{/block}}\r\n        </div>\r\n    {{/or}}\r\n\r\n    {{> components/common/modal/modal}}\r\n    {{> components/common/alert/alert-modal}}\r\n</div>\r\n","components/common/modal/modal":"<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\r\n    {{> components/common/modal/modal-close-btn}}\r\n    <div id=\"modal-content\" class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n\r\n<!-- Copy default preview Modal from product-view.html -->\r\n<div id=\"previewModal\" class=\"modal modal--large\" data-reveal>\r\n    {{> components/common/modal/modal-close-btn }}\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n","components/common/modal/modal-close-btn":"<button class=\"modal-close\"\r\n        type=\"button\"\r\n        title=\"{{lang 'common.close'}}\"\r\n        {{additional_attr}}\r\n>\r\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\r\n    <span aria-hidden=\"true\">&#215;</span>\r\n</button>\r\n"}
