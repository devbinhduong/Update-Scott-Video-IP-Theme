{"components/custom/header/header-top":"<div class=\"headerTop\">\r\n    <div class=\"container\">\r\n        <div class=\"flex flex-between flex-center-y-tablet\">\r\n            {{#if settings.phone_number}}\r\n                <div class=\"headerTop__left hide-on-tablet\">\r\n                    <p>{{settings.phone_number}}</p>\r\n                </div>\r\n            {{/if}}\r\n\r\n            <div class=\"headerTop__center\">\r\n                <p class=\"text-center-tablet\">\r\n                    {{theme_settings.header_top_center_text}}\r\n                </p>\r\n            </div>\r\n\r\n            <div class=\"headerTop__right hide-on-tablet\">\r\n                <ul class=\"flex\" style=\"--underlineColor: {{theme_settings.header_top_text_colorHover}}\">\r\n                    {{#if theme_settings.header_right_item1_text}}\r\n                        <li>\r\n                            <a class=\"hover-link\" href=\"{{theme_settings.header_right_item1_link}}\">\r\n                                <span>{{theme_settings.header_right_item1_text}}</span>\r\n                            </a>\r\n                        </li>\r\n                    {{/if}}\r\n                    {{#if theme_settings.header_right_item2_text}}\r\n                        <li>\r\n                            <a class=\"hover-link\" href=\"{{theme_settings.header_right_item2_link}}\">\r\n                                <span>{{theme_settings.header_right_item2_text}}</span>\r\n                            </a>\r\n                        </li>\r\n                    {{/if}}\r\n                    \r\n                    {{#if theme_settings.header_right_item3_text}}\r\n                        <li>\r\n                            <a class=\"hover-link\" href=\"{{theme_settings.header_right_item3_link}}\">\r\n                                <span class=\"icon icon--help\">\r\n                                    <svg><use xlink:href=\"#icon-help\"></use></svg>\r\n                                </span>\r\n                                <span>{{theme_settings.header_right_item3_text}}</span> \r\n                            </a>\r\n                        </li>\r\n                    {{/if}}\r\n                    {{#if theme_settings.header_right_item4_text}}\r\n                        <li>\r\n                            <a class=\"hover-link\"href=\"{{theme_settings.header_right_item4_link}}\">\r\n                                <span class=\"icon icon--chat\">\r\n                                    <svg><use xlink:href=\"#icon-chat\"></use></svg>\r\n                                </span>\r\n                                <span>{{theme_settings.header_right_item4_text}}</span>\r\n                            </a>\r\n                        </li>\r\n                    {{/if}}\r\n                </ul>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\n"}
